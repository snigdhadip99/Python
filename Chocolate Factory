list1=[6,0,1,8,0,2]    #input given by me
count=0
for i in range(0,len(list1)):
    if(list1[i]==0):
        count=count+1
count2=count
if(count):
    while(count!=0):
        list1.remove(0)
        count=count-1
while(count2):
    list1.append(0)
    count2=count2-1
for i in list1:
    print(i,end=" ")
